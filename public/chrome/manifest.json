{
    "name": "Code Bounty",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "A simple bounty system",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "http://*.codebounty.co/*"
    ],
    "content_scripts": [
        {
            "matches": ["https://github.com/*/*/issues/*"],
            /*"css":     ["github.css"],*/
            "js":      ["jquery-1.9.1.min.js", "github.js"],
            "run_at":  "document_end"
        }
    ],
    "homepage_url": "http://codebounty.co",
    "options_page": "options.html",
    "web_accessible_resources": []
}